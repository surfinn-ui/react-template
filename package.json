{
  "name": "vite-react-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "bin": {
    "surfinn": "./bin/surfinn.js"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "npm-run-all format:*",
    "format:md": "prettier --write \"**/*.md\"",
    "format:ts": "prettier --write \"src/**/*.ts{,x}\"",
    "gen": "npx ignite-cli generate",
    "doctor": "npx ignite-cli doctor",
    "test": "jest"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/src/**/*.{spec,test,steps}.{ts,tsx}"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "ignorePath": ".prettierignore"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/material-icons": "^4.5.4",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.3",
    "@stomp/stompjs": "^7.0.0",
    "axios-jwt": "^3.0.1",
    "date-fns": "^2.29.3",
    "i18n-js": "^4.2.2",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.1",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "mobx-state-tree": "^5.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.6.1",
    "sort-by": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/addon-storysource": "^6.5.15",
    "@storybook/builder-vite": "^0.2.7",
    "@storybook/jest": "^0.0.10",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/i18n-js": "^3.8.4",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-helmet": "^6.1.6",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "gluegun": "^5.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.4.2",
    "jest-dom": "^4.0.0",
    "prettier": "^2.8.4",
    "react-docgen-typescript": "^2.2.2",
    "storybook-addon-designs": "^6.3.1",
    "storybook-dark-mode": "^2.0.5",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-remove-console": "^1.3.0"
  }
}
